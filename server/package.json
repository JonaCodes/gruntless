{
  "name": "server",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch server.ts",
    "build": "node esbuild.config.cjs",
    "bundle": "node esbuild.config.cjs",
    "start": "node dist/server.cjs",
    "migrate": "npx sequelize-cli db:migrate --config config/config.cjs",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --config config/config.cjs",
    "seed": "npx sequelize-cli db:seed:all --config config/config.cjs",
    "create-migration": "npx sequelize-cli migration:generate --name"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.84.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "json5": "^2.2.3",
    "multer": "1.4.5-lts.2",
    "nanoid": "^5.1.6",
    "path": "^0.12.7",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.19.1",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.1",
    "globals": "^15.15.0",
    "tsx": "^4.20.6",
    "typescript": "~5.7.3"
  }
}
